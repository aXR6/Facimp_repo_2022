public static Carro{

    Carro ferrari = new Carro;

    ferrari.setPortas(4);
    ferrari.setTipo("cross");
    ferrari.setCor("azul");
    ferrari.setPlaca("psg 9i99");

    System.out.printlin("Placa",ferrari.getplaca());
    System.out.printlin("Tipo", ferrari.gettipo());
    System.out.printlin("Portas", ferrari.getportas());
    System.out.printlin("Cor", ferrari.getcor());





}   